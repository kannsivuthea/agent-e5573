[Unit]
Description=Writes E5573 stats to InfluxDB
After=network-online.target

[Service]
Type=oneshot
ExecStart=/usr/local/bin/{{ program_name }} \
            --e5573-url http://192.168.8.1 \
            --influxdb-url '{{ influxdb_url }}' \
            --verbose

[Install]
WantedBy=default.target
